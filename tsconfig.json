{
    "compilerOptions": {
        "lib": ["es2021"],
        "module": "commonjs",
        "target": "es2021",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        "removeComments": true,
    },
    "exclude": [
        "node_modules"
    ]
}
